{
  "buildCommand": "NEXT_TELEMETRY_DISABLED=1 npm run build",
  "framework": "nextjs",
  "installCommand": "npm ci",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    },
    "app/api/stripe/webhook/route.ts": {
      "maxDuration": 60
    },
    "app/api/hubspot/webhook/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "redirects": [],
  "rewrites": []
}
