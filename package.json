{
  "name": "delivery-platform",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "npm run validate",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all --parallel typecheck lint",
    "validate:ci": "npm-run-all --parallel typecheck lint",
    "validate:auth": "node scripts/validate-auth-config.js",
    "depcheck": "npx depcheck --ignores=@sentry/nextjs",
    "tsprune": "npx ts-prune",
    "analyze": "ANALYZE=true next build",
    "analyze:bundle": "ANALYZE=true npm run build",
    "sweep": "npm run typecheck && npm run lint && npm run depcheck && npm run tsprune",
    "prepare": "husky",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@hubspot/api-client": "^10.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^1.30.1",
    "@opentelemetry/instrumentation": "^0.57.2",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-google-maps/api": "^2.20.7",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9",
    "eslint-config-next": "^15.5.6",
    "lucide-react": "^0.545.0",
    "next": "^15.5.6",
    "postcss": "^8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "sonner": "^2.0.7",
    "stripe": "^14.9.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "web-push": "^3.6.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@next/bundle-analyzer": "^15.5.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "depcheck": "^1.4.5",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "ts-prune": "^0.10.3",
    "tsc-files": "^1.1.4"
  },
  "optionalDependencies": {
    "@sentry/nextjs": "^8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc-files --noEmit"
    ],
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
